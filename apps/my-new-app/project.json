{
  "root": "apps/my-new-app",
  "sourceRoot": "apps/my-new-app",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/my-new-app",
        "outputPath": "dist/apps/my-new-app"
      },
      "configurations": {
        "production": {}
      },
      "dependsOn": [
        {
          "target": "locales",
          "projects": "self"
        }
      ]
    },
    "locales": {
      "executor": "./tools/executors/copy-locales:copy-locales",
      "options": {
        "libs": ["lib1"]
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "my-new-app:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "buildTarget": "my-new-app:build:production",
          "dev": false
        }
      },
      "dependsOn": [
        {
          "target": "locales",
          "projects": "self"
        }
      ]
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "my-new-app:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/my-new-app"],
      "options": {
        "jestConfig": "apps/my-new-app/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/my-new-app/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
